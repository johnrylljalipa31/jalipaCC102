#include <stdio.h>

int main() {
    int num;
    
    printf("Enter a number: ");
    scanf("%d", &num);

    printf("Odd numbers from 1 to %d: ", num);
    for (int i = 1; i <= num; i += 2) {
        printf("%d", i);
        if (i + 2 <= num) {
            printf(", ");
        }
    }
    
    printf("\n");
    return 0;
}
